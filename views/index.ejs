<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
    <title><%= title %></title>
  </head>
  <body>
  <div class="container-fluid">
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <!-- <p>content is <%= content %></p> -->

    <table class="table table-striped">
      <thead>
        <tr>
          <th>Date</th>
          <th>Login Count</th>
          <th>Total Forum Post</th>
          <th>Nautilus Beta2 64bit Downloads</th>
          <th>Nautilus Beta2 32bit Downloads</th>
        </tr>
      </thead>
      <tbody>
        <% for(var i=0; i<content.length; i++) { %>
          <tr>
            <td><%= content[i].date %></td>
            <td><%= content[i].total_Project_Login %></td>
            <td><%= content[i].total_Forum_Posts %></td>
            <td><%= content[i].total_download_Nautilus_Beta2_x64 %></td>
            <td><%= content[i].total_download_Nautilus_Beta2_x86 %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>

    <script src="/javascripts/jquery-2.2.0.min.js"></script>
    <script src="/javascripts/bootstrap.min.js"></script>
  </body>
</html>
